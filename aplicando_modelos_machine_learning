      "cell_type": "code",
      "source": [
        "#modelo 2\n",
        "# Inicializando o modelo Gradient Boosting Classifier\n",
        "gb_classifier = GradientBoostingClassifier(random_state=42)\n",
        "\n",
        "# Treinando o modelo\n",
        "gb_classifier.fit(X_train, y_train)\n",
        "\n",
        "# Prevendo no conjunto de teste\n",
